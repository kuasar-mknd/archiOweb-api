		{
			"name": "Delete Garden",
			"event": [
				{
					"listen": "prerequest",
					"script": {
						"id": "85fa3934-1973-4474-bd65-f7212d1b2966",
						"exec": [
							"// Vérifie si l'ID est déjà défini dans les paramètres de la requête\r",
							"let requestId = pm.request.url.variables.get(\"id\");\r",
							"\r",
							"if (!requestId) {\r",
							"    const requestBody = {\r",
							"        name: \"Mon Jardin\",\r",
							"        location: {\r",
							"            type: \"Point\",\r",
							"            coordinates: [-73.856077, 40.848447]\r",
							"        }\r",
							"    };\r",
							"\r",
							"    // Exécute la requête de création de jardin et récupère l'ID\r",
							"    pm.sendRequest({\r",
							"        url: pm.environment.get(\"baseUrl\") + \"/api/gardens\",\r",
							"        method: 'POST',\r",
							"        header: {\r",
							"            'Content-Type': 'application/json',\r",
							"            'Authorization': 'Bearer ' + pm.environment.get(\"authToken\")\r",
							"        },\r",
							"        body: {\r",
							"            mode: 'raw',\r",
							"            raw: JSON.stringify(requestBody)\r",
							"        }\r",
							"    }, function (err, response) {\r",
							"        if (err) {\r",
							"            console.log(\"Erreur lors de la création du jardin :\", err);\r",
							"        } else {\r",
							"            const responseJson = response.json();\r",
							"            let newId = responseJson._id;\r",
							"\r",
							"            // Définit l'ID dans les paramètres de la requête\r",
							"            pm.request.url.variables.add({key: \"id\", value: newId});\r",
							"        }\r",
							"    });\r",
							"}\r",
							""
						],
						"type": "text/javascript"
					}
				},
				{
					"listen": "test",
					"script": {
						"id": "90d498c6-cc8b-4f83-a017-0273ef147873",
						"exec": [
							"pm.test(\"Status code is 204\", function () {\r",
							"    pm.response.to.have.status(204);\r",
							"});\r",
							"\r",
							"pm.test(\"Body is empty\", function () {\r",
							"    pm.expect(pm.response.text()).to.be.empty;\r",
							"});\r",
							""
						],
						"type": "text/javascript"
					}
				}
			],
